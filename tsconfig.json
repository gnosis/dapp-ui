{
    "compilerOptions": {
        "outDir": "lib",
        "declaration": true,
        "declarationMap": true,
        "esModuleInterop": true,
        "target": "es6",
        "downlevelIteration": true,
        "importHelpers": true,
        "sourceMap": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "jsx": "react",
        "allowJs": false,
        "checkJs": false,
        "lib": [
            "dom",
            "es6",
            "es7",
            "esnext"
        ],
        "forceConsistentCasingInFileNames": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "skipLibCheck": true,
        "strictNullChecks": false,
        "suppressImplicitAnyIndexErrors": true,
        "types": [
            "jest"
        ],
    },
    "include": [
        "src/index.tsx"
    ]
}